{
  "qualifiedName": "@llmindset/mcp-miro",
  "displayName": "MIRO Whiteboard Connector",
  "description": "Connects to the MIRO Whiteboard Application for board manipulation and sticky creation.",
  "iconUrl": "https://icons.duckduckgo.com/ip3/github.com.ico",
  "remote": false,
  "deploymentUrl": null,
  "security": null,
  "tools": [],
  "connections": [
    {
      "type": "stdio",
      "stdioFunction": "config => ({ command: 'npx', args: ['-y', '@llmindset/mcp-miro', '--token', config.miroOauthKey] })",
      "configSchema": {
        "type": "object",
        "required": [
          "miroOauthKey"
        ],
        "properties": {
          "miroOauthKey": {
            "type": "string",
            "description": "The OAuth key for accessing MIRO API services."
          }
        }
      }
    }
  ]
}