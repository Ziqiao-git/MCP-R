{
  "qualifiedName": "@dravidsajinraj-iex/ai-image-generator-mcp",
  "displayName": "Leonardo AI Image Generator",
  "description": "Generate high-quality images from text prompts using Leonardo AI's advanced models. Transform your ideas into visuals seamlessly with a simple MCP interface. Benefit from robust error handling and reliable image generation capabilities.",
  "iconUrl": "https://icons.duckduckgo.com/ip3/leonardo.ai.ico",
  "remote": true,
  "deploymentUrl": "https://server.smithery.ai/@dravidsajinraj-iex/ai-image-generator-mcp",
  "security": null,
  "tools": [
    {
      "name": "generateHeroImage",
      "description": "Generate a hero image from text",
      "inputSchema": {
        "type": "object",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "required": [
          "prompt"
        ],
        "properties": {
          "prompt": {
            "type": "string",
            "description": "Text to generate image from"
          }
        },
        "additionalProperties": false
      }
    }
  ],
  "connections": [
    {
      "type": "http",
      "deploymentUrl": "https://server.smithery.ai/@dravidsajinraj-iex/ai-image-generator-mcp/mcp",
      "configSchema": {
        "type": "object",
        "title": "MCP Session Configuration",
        "required": [
          "leonardoApiKey",
          "presignedUrl",
          "generationUrl"
        ],
        "properties": {
          "debug": {
            "type": "boolean",
            "default": false,
            "description": "Enable debug logging"
          },
          "presignedUrl": {
            "type": "string",
            "minLength": 1,
            "description": "Leonardo presigned base url"
          },
          "generationUrl": {
            "type": "string",
            "minLength": 1,
            "description": "Leonardo image generation url"
          },
          "leonardoApiKey": {
            "type": "string",
            "minLength": 1,
            "description": "Leonardo API key"
          }
        },
        "description": "Schema for the /mcp endpoint configuration",
        "x-mcp-version": "1.0",
        "x-query-style": "dot+bracket",
        "additionalProperties": false
      }
    }
  ]
}